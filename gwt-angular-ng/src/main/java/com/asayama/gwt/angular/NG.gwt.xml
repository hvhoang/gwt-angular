<?xml version="1.0" encoding="UTF-8"?>
<module>
  <inherits name="bower_components/angular" />
  <inherits name="com.asayama.gwt.jsni.JSNI" />
  <inherits name="com.asayama.gwt.jquery.JQueryLite" />
  <inherits name="com.asayama.gwt.resources.Resources" />

  <generate-with class="com.asayama.gwt.angular.rebind.DefaultInstantiableCreatorGenerator">
    <when-type-assignable class="com.asayama.gwt.angular.client.Creator" />
  </generate-with>
  <generate-with class="com.asayama.gwt.angular.rebind.DependencyInspectorGenerator">
    <when-type-assignable class="com.asayama.gwt.angular.client.DependencyInspector" />
  </generate-with>
  <generate-with class="com.asayama.gwt.angular.rebind.BinderFactoryGenerator">
    <when-type-assignable class="com.asayama.gwt.angular.client.BinderFactory" />
  </generate-with>
  <generate-with class="com.asayama.gwt.angular.rebind.ScopeBinderFactoryGenerator">
    <when-type-assignable class="com.asayama.gwt.angular.client.ScopeBinderFactory" />
  </generate-with>
  <source path="client" />
  <entry-point class="com.asayama.gwt.angular.client.NG" />

</module>
